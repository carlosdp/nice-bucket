{
  "manifest_version": 2,
    "name": "Nice Bucket",
    "version": "1.0.0",
    "description": "Filter Ice Bucket challenge posts from Facebook.",
    "icons":{
      "16": "images/bucket16.png",
      "64": "images/bucket64.png",
      "128": "images/bucket128.png"
    },
    "content_scripts":[
    {
      "matches":["https://www.facebook.com/*"],
      "js":["jquery.js", "nice-bucket.js"]
    }
    ],
    "web_accessible_resources": [
      "jquery.js",
      "nice-bucket.js"
    ]
}
